<dsx>
    <scene root="base" axis_length="20.0"/>

    <views default="cam0">
        <perspective id="cam0" near="0.1" far="500" angle="57">
            <from x="2" y="1.3" z="2.3"/>
            <to x="2" y="0" z="0.5"/>
        </perspective>
        <perspective id="cam1" near="0.1" far="500" angle="57">
            <from x="2" y="1.3" z="-0.8"/>
            <to x="2" y="0" z="0.5"/>
        </perspective>
    </views>

    <illumination doublesided="1" local="1">
        <ambient r="0.6" g="0.6" b="0.6" a="1.0"/>
        <background r="0" g="0" b="0" a="1.0"/>
    </illumination>

    <lights>
        <omni id="omni1" enabled="1">
            <location x="10" y="2.8" z="-10" w="1"/>
            <ambient r="0" g="0" b="0" a="1.0"/>
            <diffuse r="0.6" g="0.6" b="0.6" a="1"/>
            <specular r="0.4" g="0.4" b="0.4" a="1"/>
        </omni>

        <omni id="omni2" enabled="1">
            <location x="0" y="50" z="0" w="1"/>
            <ambient r="0" g="0" b="0" a="1.0"/>
            <diffuse r="0.6" g="0.6" b="0.6" a="1"/>
            <specular r="0.4" g="0.4" b="0.4" a="1"/>
        </omni>

        <spot id="umS" enabled="1" angle="10" exponent="3">
            <!-- atencao, "target" e' diferente de "direction" -->
            <target x="0.0" y="0.0" z="0.0"/>
            <location x="20" y="200" z="15"/>
            <ambient r="0.1" g="0.1" b="0.1" a="0.1"/>
            <diffuse r="0.1" g="0.1" b="0.1" a="0.1"/>
            <specular r="0.1" g="0.1" b="0.1" a="0.1"/>
        </spot>
    </lights>

    <textures>
        <texture id="metal" file="../resources/metal.png" length_s="10" length_t="10"/>
        <texture id="grass" file="../resources/grass.png" length_s="50" length_t="50"/>
        <texture id="sky" file="../resources/sky.png" length_s="100" length_t="100"/>
        <texture id="wood" file="../resources/madeira-escura.jpg" length_s="2" length_t="2"/>
        <texture id="woodchao" file="../resources/woodlight.jpg" length_s="2" length_t="2"/>
        <texture id="photo" file="../resources/edited.png" length_s="5" length_t="4"/>
        <texture id="cowboy" file="../resources/Cowboy.jpg" length_s="5" length_t="4"/>
        
    </textures>

    <materials>
        <material id="baco">
            <emission r="0" g="0" b="0" a="1.0"/>
            <ambient r="0.8" g="0.6" b="0.6" a="1.0"/>
            <diffuse r="0.8" g="0.6" b="0.6" a="1.0"/>
            <specular r="0" g="0" b="0" a="1.0"/>
            <shininess value="100"/>
        </material>

        <material id="green">
            <emission r="0" g="0.1" b="0" a="1.0"/>
            <ambient r="0.5" g="0.5" b="0.6" a="1.0"/>
            <diffuse r="0.5" g="0.6" b="0.6" a="1.0"/>
            <specular r="0" g="0" b="0" a="1.0"/>
            <shininess value="100"/>
        </material>

        <material id="metal">
            <emission r="0.4" g="0.4" b="0.5" a="1.0"/>
            <ambient r="0.6" g="0.6" b="0.8" a="1.0"/>
            <diffuse r="0.8" g="0.6" b="0.6" a="1.0"/>
            <specular r="0.5" g="0.5" b="0.7" a="1.0"/>
            <shininess value="100"/>
        </material>

    </materials>

    <transformations>
        <transformation id="null">
            <translate x="0" y="0" z="0"/>
        </transformation>

        <transformation id="base">
            <translate x="0" y="0" z="0"/>
        </transformation>
    </transformations>

    <animations>
        <animation id="ani1" span="3" type="circular" centerx="0" centery="0" centerz="0" radius="20" startang="0" rotang="360"/>
        <animation id="ani2" span="3" type="circular" centerx="0" centery="0" centerz="0" radius="20" startang="180" rotang="-360"/>
        <animation id="ani3" span="0.6" type="linear">
            <controlpoint xx="0" yy="0" zz="0"/>
            <controlpoint xx="0" yy="0" zz="20"/>
        </animation>
        <animation id="ani4" span="2" type="circular" centerx="0" centery="0" centerz="0" radius="20" startang="0" rotang="270"/>
        <animation id="ani5" span="0.6" type="linear">
            <controlpoint xx="0" yy="0" zz="0"/>
            <controlpoint xx="20" yy="0" zz="0"/>
        </animation>
        <animation id="olaaa" span="3" type="perspective" clock="1">
            <perspectiveref id="cam0"/>
            <perspectiveref id="cam1"/>
        </animation>
    </animations>

    <primitives>
        <primitive id="base">
            <rectangle x1="-8" y1="-8" x2="8" y2="8"/>
        </primitive>


        <primitive id="pho">
            <rectangle x1="-2" y1="-2" x2="2" y2="2"/>
        </primitive>


        <primitive id="paralel">
            <cylinder top="0.5" base="0.5" height="4" slices="4" stacks="2"/>
        </primitive>

        <primitive id="tank">
            <vehicle/>
        </primitive>

        <primitive id="parede">
            <plane dimX="8" dimY="16" partsX="20" partsY="13"/>
        </primitive>

         <primitive id="torus">
            <torus inner="1.1" outer="1.22" slices="15" loops="15"/>
        </primitive>

    </primitives>

    <components>

        <component id="base">
            <transformation>
                <rotate axis="x" angle="-90"/>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="sky"/>
            <children>
                <componentref id="chao"/>
                <componentref id="parede1"/>
                <componentref id="parede2"/>
                <componentref id="parede3"/>
                <componentref id="parede4"/>
                <componentref id="objects"/>
            </children>
        </component>


           <component id="objects">
            <transformation>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="none"/>
            <children>
               <componentref id="photo1"/>
               <componentref id="photo2"/>
               <componentref id="photo3"/>
               <componentref id="balcon"/>
               <componentref id="steellRing"/>
            </children>
        </component>


         <component id="steellRing">
            <transformation>
             <translate x="-5.2" y="4.1" z="-1"/>
            <rotate axis="x" angle="90"/>
             <rotate axis="z" angle="45"/> 
            </transformation>
            <materials>
                <material id="metal"/>
            </materials>
            <texture id="metal"/>
            <children>
                <primitiveref id="torus"/>
            </children>
        </component>

          <component id="balcon">
            <transformation>
          
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="none"/>
            <children>
                <componentref id="frontBalcon"/>
                <componentref id="topBalcon"/>
            </children>
        </component>



         <component id="frontBalcon">
            <transformation>
             <translate x="-5" y="4" z="-1"/>
             <scale x="0.3" y="3" z="4"/>  
             <rotate axis="x" angle="90"/>
             <rotate axis="z" angle="45"/>     
                         
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="woodchao"/>
            <children>
                <primitiveref id="paralel"/>
            </children>
        </component>


         <component id="topBalcon">
            <transformation>
            <translate x="-5" y="-2" z="0.2"/>
             <scale x="1" y="17" z="0.2"/>  
             <rotate axis="z" angle="45"/>   
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="woodchao"/>
            <children>
                <primitiveref id="paralel"/>
            </children>
        </component>

          <component id="photo1">
            <transformation>
             <translate x="-7.9" y="0" z="2"/>
              
             <scale x="0.3" y="0.3" z="0.3"/>
             <rotate axis="x" angle="90"/>
             <rotate axis="y" angle="90"/>
            
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="photo"/>
            <children>
                <componentref id="image"/>
                <componentref id="border"/>
            </children>
        </component>
        

          <component id="photo2">
            <transformation>
             <translate x="-7.9" y="4" z="2"/>
              
             <scale x="0.3" y="0.3" z="0.3"/>
             <rotate axis="x" angle="90"/>
             <rotate axis="y" angle="90"/>
            
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="cowboy"/>
            <children>
                <componentref id="image2"/>
                <componentref id="border"/>
            </children>
        </component>

          <component id="photo3">
            <transformation>
             <translate x="-7.9" y="-4" z="2"/>
              
             <scale x="0.3" y="0.3" z="0.3"/>
             <rotate axis="x" angle="90"/>
             <rotate axis="y" angle="90"/>
            
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="cowboy"/>
            <children>
                <componentref id="image2"/>
                <componentref id="border"/>
            </children>
        </component>

        <component id="image2">
            <transformation>
             
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="cowboy"/>
            <children>
                <primitiveref id="pho"/>
            </children>
        </component>
        


            <component id="image">
            <transformation>
             
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="photo"/>
            <children>
                <primitiveref id="pho"/>
            </children>
        </component>

            <component id="border">
            <transformation>
             
            <rotate axis="y" angle="90"/>
            
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <componentref id="b1"/>
                <componentref id="b2"/>
                <componentref id="b3"/>
                <componentref id="b4"/>
            </children>
        </component>
    
             <component id="b1">
            <transformation>
             <translate x="0" y="2" z="-2"/>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <primitiveref id="paralel"/>
            </children>
        </component>

            <component id="b2">
            <transformation>
             <translate x="0" y="-2" z="-2"/>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <primitiveref id="paralel"/>
            </children>
        </component>

         <component id="b3">
            <transformation>
             <translate x="0" y="2" z="-2"/>
              <rotate axis="x" angle="90"/>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <primitiveref id="paralel"/>
            </children>
        </component>
         <component id="b4">
            <transformation>
             <translate x="0" y="2" z="2"/>
              <rotate axis="x" angle="90"/>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <primitiveref id="paralel"/>
            </children>
        </component>

        <component id="chao">
            <transformation>
                <translate x="0" y="0" z="-2.1"/>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <primitiveref id="base"/>
            </children>
        </component>

        <component id="parede1">
            <transformation>
                <translate x="-8" y="0" z="2"/>
                <rotate axis="y" angle="90"/>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <primitiveref id="parede"/>
            </children>
        </component>

        <component id="parede2">
            <transformation>
                <translate x="8" y="0" z="2"/>
                <rotate axis="y" angle="-90"/>

            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <primitiveref id="parede"/>
            </children>
        </component>

        <component id="parede3">
            <transformation>
                <translate x="0" y="8" z="2"/>
                <rotate axis="y" angle="90"/>
                <rotate axis="x" angle="90"/>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <primitiveref id="parede"/>
            </children>
        </component>

        <component id="parede4">
            <transformation>
                <translate x="0" y="-8" z="2"/>
                <rotate axis="y" angle="90"/>
                <rotate axis="x" angle="-90"/>
            </transformation>
            <materials>
                <material id="baco"/>
            </materials>
            <texture id="wood"/>
            <children>
                <primitiveref id="parede"/>
            </children>
        </component>




    </components>
</dsx>
